<apex:page controller="Sobject_Examples">
    <apex:form >
    <apex:pageBlock title="Account">
        <apex:pageBlockSection columns="1">
            <apex:outputText value="{!acc.Name}"/>
            <apex:outputText value="{!acc.Phone}"/>
            <apex:outputText value="{!acc.Industry}"/>
            <apex:outputText value="{!acc.Rating}"/>
        </apex:pageBlockSection>
        </apex:pageBlock>
        
        <apex:pageBlock title="Contact">
        <apex:pageBlockSection columns="1">
            <apex:inputText value="{!con.lastName}"/>
            <apex:inputText value="{!con.firstName}"/>
            <apex:inputText value="{!con.Phone}"/>
            <apex:inputText value="{!con.email}"/>
        </apex:pageBlockSection>
        </apex:pageBlock>
        
    </apex:form>
</apex:page>